LOAD STUDENTS
LOAD ADVISOR
MID_Q3_1 <- CROSS STUDENTS ADVISOR
MID_Q3_2 <- SELECT adv_id == advisor_id FROM MID_Q3_1
MID_Q3_3 <- SELECT adv_sex != stud_sex FROM MID_Q3_2
ANS_Q3 <- PROJECT stud_id,adv_id,stud_sex,adv_sex FROM MID_Q3_3
PRINT ANS_Q3
CLEAR ANS_Q3
CLEAR MID_Q3_1
CLEAR MID_Q3_2
CLEAR MID_Q3_3
CLEAR STUDENTS
CLEAR ADVISOR