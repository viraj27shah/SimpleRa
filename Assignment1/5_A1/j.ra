LOAD EMPLOYEE
MID_J <- CROSS EMPLOYEE EMPLOYEE
MID1_J <- SELECT EMPLOYEE1_Super_ssn == EMPLOYEE2_Ssn FROM MID_J
MID2_J <- SELECT EMPLOYEE1_Bdate == EMPLOYEE2_Bdate FROM MID1_J
ANS_J <- PROJECT EMPLOYEE1_Ssn, EMPLOYEE1_Super_ssn, EMPLOYEE1_Bdate FROM MID2_J
RENAME EMPLOYEE1_Ssn TO Ssn FROM ANS_J
RENAME EMPLOYEE1_Super_ssn TO Super_ssn FROM ANS_J
RENAME EMPLOYEE1_Bdate TO Bdate FROM ANS_J
PRINT ANS_J
CLEAR EMPLOYEE
CLEAR MID1_J
CLEAR MID2_J
CLEAR MID_J
CLEAR ANS_J