LOAD STUDENTS
LOAD ADVISOR
MID_Q7_1 <- SELECT adv_age > 40 FROM ADVISOR
MID_Q7_2 <- CROSS STUDENTS MID_Q7_1
MID_Q7_3 <- SELECT advisor_id == adv_id FROM MID_Q7_2
ANS_Q7 <- PROJECT stud_id,adv_id FROM MID_Q7_3
PRINT ANS_Q7
CLEAR ANS_Q7
CLEAR MID_Q7_1
CLEAR MID_Q7_2
CLEAR MID_Q7_3
CLEAR STUDENTS
CLEAR ADVISOR